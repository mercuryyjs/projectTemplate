{
    "extends": "standard",
    "rules": {
      "no-var": 0,
      "semi": 0,
      "spaced-comment": 0,
      "require-jsdoc": [2, {
        "require": {
          "FunctionDeclaration": true,
          "MethodDefinition": false,
          "ClassDeclaration": false
        }
      }],
      "no-alert": 2

    }
}
